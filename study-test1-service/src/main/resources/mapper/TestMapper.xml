<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.big.data.dao.TestMapper">
    <!-- Result Map -->
    <resultMap id="BaseResultMap" type="com.big.data.entity.TestBean">
        <result column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="made_status" property="madeStatus"/>
    </resultMap>


    <select id="getNameById" resultType="com.big.data.entity.TestBean">
		SELECT * FROM  tb1 WHERE id=#{id}
	</select>


</mapper>   
